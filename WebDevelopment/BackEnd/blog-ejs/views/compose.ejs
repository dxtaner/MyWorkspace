<%- include("partials/header") %>

<h1>Compose New Post</h1>

<form action="/compose" method="POST">
  <input type="text" name="postTitle" placeholder="Title" required />
  <textarea
    name="postBody"
    rows="10"
    placeholder="Write your post..."
    required
  ></textarea>
  <button type="submit">Publish</button>
</form>

